<float name="Firing Patterns.Moving Rate of Fire" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x120" visible="true"/>
<float name="Firing Patterns.Moving Projectile Error" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x124" visible="true"/>
<float name="Firing Patterns.Berserk Burst Duration" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x130" visible="true"/>
<float name="Firing Patterns.Berserk Burst Separation" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x134" visible="true"/>
<float name="Firing Patterns.Berserk Rate of Fire" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x138" visible="true"/>
<float name="Firing Patterns.Berserk Projectile Error" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x13C" visible="true"/>

<float name="Special-Case Firing Properties.Super-Ballistic Range" note="" info="aim shots super-ballistically if target is outside this range" info_img="" offset="0x148" visible="true"/>
<float name="Special-Case Firing Properties.Bombardment Range" note="" info="offset burst targets randomly by this range when firing at non-visible enemies" info_img="" offset="0x14C" visible="true"/>
<float name="Special-Case Firing Properties.Modified Vision Range" note="" info="any custom vision range this actv has" info_img="" offset="0x150" visible="true"/>

<enum16 name="Special-Case Firing Properties.Special-Fire Mode" note="" info="type of special weapon fire possible" info_img="" offset="0x154" visible="true">
	<option name="None" value="00"/>
	<option name="Overcharge" value="01"/>
	<option name="Secondary Trigger" value="02"/>
</enum16>

<enum16 name="Special-Case Firing Properties.Special-Fire Situation" note="" info="when to use special weapon mode" info_img="" offset="0x156" visible="true">
	<option name="Never" value="00"/>
	<option name="Enemy Visible" value="01"/>
	<option name="Enemy Out of Sight" value="02"/>
	<option name="Strafing" value="03"/>
</enum16>

<float name="Special-Case Firing Properties.Special-Fire Chance" note="0 - 1 only" info="how likely to use this" info_img="" offset="0x158" visible="true"/>
<float name="Special-Case Firing Properties.Special-Fire Delay" note="seconds" info="waiting period between mode uses" info_img="" offset="0x15C" visible="true"/>

<float name="Berserking and Melee.Melee Range" note="world units" info="" info_img="" offset="0x160" visible="true"/>
<float name="Berserking and Melee.Melee Abort Range" note="world units" info="" info_img="" offset="0x164" visible="true"/>
<float name="Berserking and Melee.Berserk Firing Range: from" note="world units" info="" info_img="" offset="0x168" visible="true"/>
<float name="Berserking and Melee.Berserk Firing Range: to" note="world units" info="" info_img="" offset="0x16C" visible="true"/>
<float name="Berserking and Melee.Berserk Melee Range" note="world units" info="" info_img="" offset="0x170" visible="true"/>
<float name="Berserking and Melee.Berserk Melee Abort Range" note="world units" info="" info_img="" offset="0x174" visible="true"/>

<enum16 name="Grenades.Grenade Type" note="" info="type of grenades thrown" info_img="" offset="0x180" visible="true">
	<option name="Human Fragmentation" value="00"/>
	<option name="Covenant Plasma" value="01"/>
</enum16>

<enum16 name="Grenades.Trajectory Type" note="" info="how grenades are thrown" info_img="" offset="0x182" visible="true">
	<option name="Toss" value="00"/>
	<option name="Lob" value="01"/>
	<option name="Bounce" value="02"/>
</enum16>

<enum16 name="Grenade.Grenade Stimulus" note="" info="what causes us to consider throwing a grenade" info_img="" offset="0x184" visible="true">
	<option name="Never" value="00"/>
	<option name="Visible Target" value="01"/>
	<option name="Seek Cover" value="02"/>
</enum16>

<short name="Grenades.Minimum Enemy Count" note="" info="how many enemies must be within the radius of the grenade before we consider throwing it there" info_img="" offset="0x186" visible="true"/>

<float name="Grenades.Enemy Radius" note="world units" info="consider enemies within this radius of throw location" info_img="" offset="0x188" visible="true"/>
<float name="Grenades.Grenade Velocity" note="world units per second" info="how fast to throw the grenades" info_img="" offset="0x190" visible="true"/>
<float name="Grenades.Grenade Ranges: from" note="world units" info="ranges within which to consider throwing grenade" info_img="" offset="0x194" visible="true"/>
<float name="Grenades.Grenade Ranges: to" note="world units" info="ranges within which to consider throwing grenade" info_img="" offset="0x198" visible="true"/>
<float name="Grenades.Collateral Damage Radius" note="world units" info="don't throw if friendlies are within this range of target" info_img="" offset="0x19C" visible="true"/>
<float name="Grenades.Grenade Chance" note="0 - 1 only" info="how likely to throw a grenade" info_img="" offset="0x1A0" visible="true"/>
<float name="Grenades.Grenade Check Time" note="seconds" info="how often to consider throwing a grenade while stimulated" info_img="" offset="0x1A4" visible="true"/>
<float name="Grenades.Encounter Grenade Timeout" note="seconds" info="don't throw grenade if another actor within my encounter threw one this recently" info_img="" offset="0x1A8" visible="true"/>

<dependency name="Items.Equipment" note="" info="drops upon death" info_img="" offset="0x1C0" visible="true"/>

<short name="Items.Grenade Count: from" note="" info="number of grenades that we start with" info_img="" offset="0x1D0" visible="true"/>
<short name="Items.Grenade Count: to" note="" info="number of grenades that we start with" info_img="" offset="0x1D2" visible="true"/>

<float name="Items.Don't Drop Grenades Chance " note="0 - 1 only" info="how likely to NOT drop grenades, even if we still have some" info_img="" offset="0x1D4" visible="true"/>
<float name="Items.Drop Weapon Loaded: from" note="fractions of a clip, eg. 0.3 to 0.5" info="" info_img="" offset="0x1D8" visible="true"/>
<float name="Items.Drop Weapon Loaded: to" note="fractions of a clip, eg. 0.3 to 0.5" info="amount of ammo loaded into the weapon that drops" info_img="" offset="0x1DC" visible="true"/>

<short name="Items.Drop Weapon Ammo: from" note="" info="total ammo in the dropped weapon" info_img="" offset="0x1E0" visible="true"/>
<short name="Items.Drop Weapon Ammo: to" note="" info="total ammo in the dropped weapon" info_img="" offset="0x1E2" visible="true"/>

<float name="Unit.Body Vitality" note="" info="maximum body vitality of the unit" info_img="" offset="0x200" visible="true"/>
<float name="Unit.Shield Vitality" note="" info="maximum shield vitality of the unit" info_img="" offset="0x204" visible="true"/>
<float name="Unit.Shield Sapping Radius" note="world units" info="how far away we can drain the player's shields" info_img="" offset="0x208" visible="true"/>

<short name="Unit.Force Shader Permutation" note="" info="if nonzero, overrides the unit's shader permutation" info_img="" offset="0x20C" visible="true"/>

<struct name="Change Colors" note="" info="" info_img="" offset="0x22C" visible="true" size="32">
	<colorRGB name="Color Lower Bound" note="" info="" info_img="" offset="0x00" visible="true"/>
	<colorRGB name="Color Upper Bound" note="" info="" info_img="" offset="0x0C" visible="true"/>
</struct>

</plugin>