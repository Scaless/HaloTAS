<float name="Burst Geometry.Special Damage Modifier" note="0 - 1 only" info="" info_img="actv Burst Geometry.png" offset="0xF8" visible="true"/>
<float name="Burst Geometry.Special Projectile Error" note="radians" info="" info_img="actv Burst Geometry.png" offset="0xFC" visible="true"/>

<float name="Firing Patterns.New-Target Burst Duration" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x100" visible="true"/>
<float name="Firing Patterns.New-Target Burst Separation" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x104" visible="true"/>
<float name="Firing Patterns.New-Target Rate of Fire" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x108" visible="true"/>
<float name="Firing Patterns.New-Target Projectile Error" note="multiplier; 0 = unchanged" info="" info_img="actv Firing Patterns.png" offset="0x10C" visible="true"/>
