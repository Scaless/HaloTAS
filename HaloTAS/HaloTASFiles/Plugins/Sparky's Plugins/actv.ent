<plugin class="actv" author="sparky" version="4.0" headersize="Main">

<bitmask32 name="Flags" note="" info="" info_img="" offset="0x00" visible="true">
	<option name="Can Shoot While Flying" value="31"/>
	<option name="Interpolate Color in HSV" value="30"/>
	<option name="Has Unlimited Grenades" value="29"/>
	<option name="Moveswitch Stay w/ Friends" value="28"/>
	<option name="Active Camouflage" value="27"/>
	<option name="Super Active Camouflage" value="26"/>
	<option name="Cannot Use Ranged Weapons" value="25"/>
	<option name="Prefer Passenger Seat" value="24"/>
</bitmask32>

<dependency name="Actor Definition " note="" info="" info_img="" offset="0x04" visible="true"/>
<dependency name="Unit" note="biped, unit or vehicle" info="" info_img="" offset="0x14" visible="true"/>
<dependency name="Major Variant" note="" info="" info_img="" offset="0x24" visible="true"/>

<enum16 name="Movement Switching.Movement Type " note="" info="" info_img="actv Movement Switching.png" offset="0x4C" visible="true">
	<option name="Always Run" value="00"/>
	<option name="Always Crouch" value="01"/>
	<option name="Switch Types" value="02"/>
</enum16>
 
<float name="Movement Switching.Initial Crouch Chance" note="0 - 1 only" info="actors try to maintain this fraction of crouched actors" info_img="actv Movement Switching.png" offset="0x50" visible="true"/>
<float name="Movement Switching.Crouch Time: from" note="seconds" info="how long to stay crouched before running" info_img="actv Movement Switching.png" offset="0x54" visible="true"/>
<float name="Movement Switching.Crouch Time: to" note="seconds" info="how long to stay crouched before running" info_img="actv Movement Switching.png" offset="0x58" visible="true"/>
<float name="Movement Switching.Run Time: from" note="seconds" info="how long to run before moving to a crouch" info_img="actv Movement Switching.png" offset="0x5C" visible="true"/>
<float name="Movement Switching.Run Time: to" note="seconds" info="how long to run before moving to a crouch" info_img="actv Movement Switching.png" offset="0x60" visible="true"/>

<dependency name="Ranged Combat.Weapon" note="" info="" info_img="" offset="0x64" visible="true"/>

<float name="Ranged Combat.Maximum Firing Distance" note="world units" info="only fire at targets within this distance" info_img="" offset="0x74" visible="true"/>
