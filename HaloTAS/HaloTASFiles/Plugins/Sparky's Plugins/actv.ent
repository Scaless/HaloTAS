<float name="Ranged Combat.Surprise Fire-Wildly Time" note="seconds" info="" info_img="" offset="0x90" visible="true"/>
<float name="Ranged Combat.Death Fire-Wildly Chance" note="0 - 1 only" info="" info_img="" offset="0x94" visible="true"/>
<float name="Ranged Combat.Death Fire-Wildly Time" note="seconds" info="" info_img="" offset="0x98" visible="true"/>
<float name="Ranged Combat.Desired Combat Range: from" note="world units" info="" info_img="" offset="0x9C" visible="true"/>
<float name="Ranged Combat.Desired Combat Range: to" note="world units" info="" info_img="" offset="0xA0" visible="true"/>
<float name="Ranged Combat.Custom Stand Gun Offset: i" note="" info="overrides the default in the base actor" info_img="" offset="0xA4" visible="true"/>
<float name="Ranged Combat.Custom Stand Gun Offset: i" note="" info="overrides the default in the base actor" info_img="" offset="0xA8" visible="true"/>
<float name="Ranged Combat.Custom Stand Gun Offset: k" note="" info="overrides the default in the base actor" info_img="" offset="0xAC" visible="true"/>
<float name="Ranged Combat.Custom Crouch Gun Offset: i" note="" info="overrides the default in the base actor" info_img="" offset="0xB0" visible="true"/>
<float name="Ranged Combat.Custom Crouch Gun Offset: i" note="" info="overrides the default in the base actor" info_img="" offset="0xB4" visible="true"/>
<float name="Ranged Combat.Custom Crouch Gun Offset: k" note="" info="overrides the default in the base actor" info_img="" offset="0xB8" visible="true"/>
<float name="Ranged Combat.Target Tracking" note="0 - 1 only" info="how much to follow the target while shooting" info_img="" offset="0xBC" visible="true"/>
<float name="Ranged Combat.Target Leading" note="0 - 1 only" info="lead prediction accuracy" info_img="" offset="0xC0" visible="true"/>
<float name="Ranged Combat.Weapon Damage Modifier" note="0 = default" info="fraction of normal damage to inflict" info_img="" offset="0xC4" visible="true"/>
<float name="Ranged Combat.Damage Per Second" note="used if WDM = 0" info="how much damage to inflict on target per second while firing a burst at them" info_img="" offset="0xC8" visible="true"/>

<float name="Burst Geometry.Burst Origin Radius" note="world units" info="" info_img="actv Burst Geometry.png" offset="0xCC" visible="true"/>
<float name="Burst Geometry.Burst Origin Angle" note="radians" info="" info_img="actv Burst Geometry.png" offset="0xD0" visible="true"/>
