        <Rectangle Fill="DarkOrange" HorizontalAlignment="Stretch" Stroke="Black" VerticalAlignment="Stretch" />
        <TextBlock x:Name="TBFrameCounter" HorizontalAlignment="Left" TextWrapping="Wrap" Text="123456" VerticalAlignment="Top" Margin="3,3,0,0"/>
        <TextBox x:Name="TxtCheckpoint" HorizontalAlignment="Left" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" Width="80" Margin="46,1,0,0" Height="19"/>
        <TextBox x:Name="TxtTick" HorizontalAlignment="Left" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" Width="80" Margin="126,1,0,0" Height="19"/>
        <TextBox x:Name="TxtMouseYaw" HorizontalAlignment="Left" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" Width="80" Margin="206,1,0,0" Height="19"/>
        <TextBox x:Name="TxtMousePitch" HorizontalAlignment="Left" TextWrapping="NoWrap" Text="TextBox" VerticalAlignment="Top" Width="80" Margin="286,1,0,0" Height="19"/>
    </Grid>
</UserControl>
